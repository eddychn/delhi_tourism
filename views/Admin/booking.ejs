            <div class="row">
                <div class="col-md-12">
                    <div class="card card-box">
                        <div class="card-body ">
                            <div class="table-scrollable">
                                <table class="table table-hover table-checkable order-column full-width" id="example4">
                                    <thead>
                                        <tr>
                                            <th class="center"> Name </th>
                                            <th class="center"> Mobile </th>
                                            <th class="center"> Email </th>
                                            <th class="center"> Arrive </th>
                                            <th class="center"> Depart </th>
                                            <th class="center"> Room Type </th>
                                            <th class="center"> Payment </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for(let i=0;i< hotels.length;i++){%>
                                        <tr class="odd gradeX">
                                            <td class="user-circle-img">
                                                <img src="Admin_Assets/assets/user1.jpg" alt="">
                                            </td>
                                            <td class="center"><%=hotels[i].firstname%> <%=hotels[i].lastname%></td>
                                            <td class="center"><a href="tel:<%=hotels[i].contact%>">
                                                <%=hotels[i].contact%> </a></td>
                                            <td class="center"><a href="mailto:<%=hotels[i].email%> ">
                                                <%=hotels[i].email%></a></td>
                                            <td class="center"><%=hotels[i].checkin%></td>
                                            <td class="center"><%=hotels[i].checkout%></td>
                                            <td class="center"><%=hotels[i].type%></td>
                                            <td class="center">
                                                <i class="fas fa-2x fa-check-circle" style="color: green;"></i>
                                            </td>
                                        </tr>
                                        <%}%>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
