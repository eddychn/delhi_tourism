    <!-- BOOKPASS FORM -->

    <div class="wrapper">
        <div class="title">
            Book Pass Now
        </div>
        <div class="form" id="bottom">
        <form action="/bookticket" method="POST" class="needs-validation" novalidate>
            
                <div class="inputfield">
                    <label>First Name</label>
                    <input type="text" class="input" name="firstname" value="<%=user.firstname%>" required>
                </div>
                <div class="inputfield">
                    <label>Last Name</label>
                    <input type="text" class="input" name="lastname" value="<%=user.lastname%>" required>
                </div>
                <div class="inputfield">
                    <label>Email Address</label>
                    <input type="text" name="email" class="input" value="<%=user.username%>" required>
                </div>
                <div class="inputfield">
                    <label>No of passes(Adult)</label>
                    <div class="custom_select">
                        <select name="no_adult" required>
                      <option value="">Select</option>
                      <option value="1">1</option>
                      <option value="2">2</option>
                      <option value="2">3</option>
                      <option value="2">4</option>
                      <option value="2">5</option>
                    </select>
                    </div>
                </div>

                <div class="inputfield">
                    <label>Time slot</label>
                    <div class="custom_select">
                        <select name="time_slot" required>
                    <option value="">Select</option>
                    <option value="9am to 12pm">9am to 12pm</option>
                    <option value="12pm to 3pm">12pm to 3pm</option>
                    <option value="3pm to 6pm">3pm to 6pm</option>
                    <option value="6pm to 9pm">6pm to 9pm</option>
          
                  </select>
                    </div>
                </div>

                <div class="inputfield">
                    <label>Date</label>
                    <input type="Date" class="input" name="date" required>
                </div>
                <div class="inputfield">
                    <label>No of passes(Children)</label>
                    <div class="custom_select">
                        <select name="no_children" required>
                    <option value="">Select</option>
                    <option value="1">1</option>
                    <option value="2">2</option>
                    <option value="2">3</option>
                    <option value="2">4</option>
                    <option value="2">5</option>
                  </select>
                    </div>
                </div>
                <div class="inputfield">
                    <label>Phone Number</label>
                    <input type="tel" class="input" name="contact" value="<%=user.contact%>" required>
                </div>




                <div class="inputfield">
                    <input type="submit" value="Book Now" class="btn">
                </div>
        </form>
        </div>
    </div>

